    {
      "cell_type": "code",
      "source": [
        "import numpy as np\n",
        "import matplotlib.pyplot as plt\n",
        "from mpl_toolkits.mplot3d import Axes3D\n",
        "\n",
        "\n",
        "# Coeficientes de las ecuaciones\n",
        "coefficients = np.array([\n",
        "    [-3, 9, -10],\n",
        "    [-1, -6, 4],\n",
        "    [-10, -2, 8]\n",
        "])\n",
        "\n",
        "# Términos constantes\n",
        "constants = np.array([24, -4, 20])\n",
        "\n",
        "# Resolución del sistema de ecuaciones\n",
        "solution = np.linalg.solve(coefficients, constants)\n",
        "\n",
        "print(\"Solución:\", solution)\n",
        "\n",
        "# Graficar\n",
        "fig = plt.figure()\n",
        "ax = fig.add_subplot(111, projection='3d')\n",
        "\n",
        "# Puntos de intersección\n",
        "x = solution[0]\n",
        "y = solution[1]\n",
        "z = solution[2]\n",
        "\n",
        "# Plot de los puntos de intersección\n",
        "ax.scatter(x, y, z, color='r', marker='o', s=100)\n",
        "\n",
        "# Ecuaciones de los planos\n",
        "x_values = np.linspace(-10, 10, 10)\n",
        "y_values = np.linspace(-10, 10, 10)\n",
        "X, Y = np.meshgrid(x_values, y_values)\n",
        "\n",
        "Z1 = (24 + 3*X - 9*Y) / 10\n",
        "Z2 = (-4 + X + 6*Y) / 4\n",
        "Z3 = (20 + 10*X + 2*Y) / 8\n",
        "\n",
        "# Plot de los planos\n",
        "ax.plot_surface(X, Y, Z1, alpha=0.5)\n",
        "ax.plot_surface(X, Y, Z2, alpha=0.5)\n",
        "ax.plot_surface(X, Y, Z3, alpha=0.5)\n",
        "\n",
        "ax.set_xlabel('X')\n",
        "ax.set_ylabel('Y')\n",
        "ax.set_zlabel('Z')\n",
        "\n",
        "plt.show()"
      ],
